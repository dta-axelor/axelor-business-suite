<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
<grid model="com.axelor.apps.base.db.ProductCategory" title="Product category" name="product-category-grid">
<field name="gstrate"></field>
</grid>	


 <form name="product-category-form"  id="product-cat-form" title="Product category" model="com.axelor.apps.base.db.ProductCategory">
   <toolbar>
			<button name="viewProductsBtn" title="View Products" onClick="action-view-product-by-category"/>
		</toolbar>
		<panel name="mainPanel" >
        	<field name="name"/>
        	<field name="code" x-bind="{{code|unaccent|uppercase}}" />
	        <field name="parentProductCategory" form-view="product-category-form" grid-view="product-family-grid"/>
	          <panel name="p-cat">
	          <field name="gstrate" title="GSTRate"></field>
   		</panel>

 
</panel>
</form>
 </object-views>
	